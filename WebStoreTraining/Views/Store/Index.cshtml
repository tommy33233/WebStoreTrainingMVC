@model IEnumerable<WebStoreTraining.Models.Category>

@{
    ViewBag.Title = "Index";
}

<h2>Browse Category</h2>

<p>
    Select from @Model.Count()
</p>
<ul>
    @foreach (var category in Model)
    {
        <li>
          @Html.ActionLink(category.Name,"Browse",new {category=category.Name})
        </li>
    }
</ul>

